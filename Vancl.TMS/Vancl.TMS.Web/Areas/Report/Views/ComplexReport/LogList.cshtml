@model List<Vancl.TMS.Model.Log.DeliveryFlowLogModel>
@{
    ViewBag.Title = "提货单操作日志";
    Layout = "~/Views/Shared/_Layout_Frame.cshtml";
}
@section header{
}
<div class="DataGirdPanel">
    <table cellpadding="0" cellspacing="0" border="0" class="DataTable tablelist" style="min-width: 0"
        id="ComplexReportTable">
        <thead>
            <tr>
                <th style="width: 30px">
                    序号
                </th>
                <th>
                    提货单号
                </th>
                <th>
                    操作人
                </th>
                <th>
                    操作部门
                </th>
                <th>
                    操作公司
                </th>
                <th>
                    操作类型
                </th>
                <th>
                    描述
                </th>
                @*<th>
                    当前状态
                </th>*@
                <th>
                    操作时间
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model == null || Model.Count == 0)
            {
                Html.RenderPartial("_PartialDataTableEmptyRow", new List<String>());
            }
            else
            {
                for (int i = 0; i < Model.Count; i++)
                {
                <tr>
                    <td class="center">
                        @(i + 1)
                    </td>
                    <td class="center">
                        @Model[i].DeliveryNO
                    </td>
                    <td class="center">
                        @Model[i].Operator
                    </td>
                    <td>
                        @Model[i].OperateDeptName
                    </td>
                    <td>
                        @Model[i].OperateCompanyName
                    </td>
                    <td class="center">
                        @Vancl.TMS.Util.EnumUtil.EnumHelper.GetDescription(Model[i].FlowType);
                    </td>
                    <td>
                        @Model[i].Note
                    </td>
                    @*<td class="center">
                        @Vancl.TMS.Util.EnumUtil.EnumHelper.GetDescription(Model[i].DeliveryStatus.Value);
                    </td>*@
                    <td class="center">
                        @Model[i].OperateTime
                    </td>
                </tr>
                }
            }
        </tbody>
    </table>
</div>
