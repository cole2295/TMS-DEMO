@{
    ViewBag.Title = "转站入库";
    Layout = "~/Views/Shared/_Layout_Frame.cshtml";
}
@section header{
    <script src="@Url.Content("~/Scripts/references/jQuery.tmpl.min.js")  " type="text/javascript"></script>
    <script id="tmpSimpleList" type="text/html">
			<tr>
				<td>
					${FormCode}
				</td>
				<td>
					${CustomerOrder}
				</td>
			</tr>
    </script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/pages/Sorting/TurnInbound.js?" + DateTime.Now.ToString("yyyyMMdd"))"></script>
    <style type="text/css">
        .tb_insert tr th
        {
            width: 130px;
            text-align: right;
        }
        .cur
        {
            font-size: large !important;
            font-weight: bolder !important;
        }
    </style>
}
<div class="search_condition" id="divSearchCondition">
    <table class="tb_insert">
        <tr id="trSortingCenterSelect">
            <th>
                分拣站点：
            </th>
            <td>
                @{Html.RenderPartial("CityAndStation");}
            </td>     
        </tr>
        <tr>
            <th>
                今日扫描成功入库数:
            </th>
            <td>
                <span id="CurrentOpCount" style="font-size: 30px; font-weight: bold;">0</span>
            </td>
        </tr>
        <tr>
            <th>
                <select id="selFormType" class="ScanInput">
                    <option value="0" selected="selected">运单号</option>
                    <option value="1">订单号</option>
                    <option value="2">配送单号</option>
                </select>
            </th>
            <td>
                <input type="text" id="txtFormCode" class="ScanInput" value="" />
            </td>
        </tr>
    </table>
</div>
<div class="DataGirdPanel">
    <div class="tablelist">
        <table cellpadding="0" cellspacing="0" border="0" class="DataTable center" id="tblBillInfo">
            <thead>
                <tr>
                    <th>
                        运单号
                    </th>
                    <th>
                        订单号
                    </th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
@{Html.RenderPartial("_wavMedia");}
<input type="hidden" id="hdInitData" value="@ViewBag.HiddenValue" />




