@model List<Vancl.TMS.Model.Sorting.Return.BillReturnDetailInfoModel>
<div class="toolbar">
    <ul>
        <li class="toolbtn">@Ajax.ActionLink("刷新", "Index", null, (AjaxOptions)ViewBag.AjaxOptions, new { @class = "refresh", id = "btnListRefresh" })</li>
        <li class="toolbtn"><a href="javascript:ReturnOutBound();" class="expand" id="btnReturnBound">退货出库</a></li>
        <li class="toolbtn"><a href="javascript:DeleteBill();" class="del" id="btnDeleteBill" >删除运单</a></li>
        <li class="toolbtn"><a href="javascript:showReceipt();" class="print" id="btnInBoxPrint">装箱打印</a></li>
        <li class="toolbtn"><a href="javascript:ResetPage();" class="reset" id="btnReset">重置页面</a></li>

    </ul>
</div>
<div class="tablelist">
    <table cellpadding="0" cellspacing="0" border="0"  class="DataTable center"
        id="tblBillInfo">
        <thead>
            <tr>
                <th class="CbxColumn">
                    <input type="checkbox" class="CheckAll" />
                </th>
                <th >
                    箱号
                </th>
                <th>
                    运单号
                </th>
                <th>
                    订单号
                </th>
                <th>
                    称重重量(kg)
                </th>
                <th>
                    返货目的地
                </th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
