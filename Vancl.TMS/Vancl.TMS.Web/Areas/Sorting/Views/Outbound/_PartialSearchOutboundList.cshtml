@model Vancl.TMS.Model.Sorting.Outbound.ViewOutboundSearchListModel
<div class="toolbar">
    <ul>
        <li class="toolbtn">@Ajax.ActionLink("刷新", "SearchOutbound", null, (AjaxOptions)ViewBag.AjaxOptions, new { @class = "refresh", id = "btnListRefresh" })</li>
        <li class="toolbtn"><a href="javascript:SearchOutbound();" class="expand">出库</a></li>
    </ul>
    @if (Model != null)
    {
        <div id="InboundingCount" class="blue fright">
            【总数量】@(Model.FormCodeList == null ? "0" : Model.FormCodeList.Count.ToString()) &nbsp;&nbsp;&nbsp;
            【入库中数量】<span class="red">@Model.InboundingCount</span>
        </div>
    }
</div>
<div class="tablelist">
    <table cellpadding="0" cellspacing="0" border="0" class="DataTable MultiSelect"  id="tblBillInfo">
        <thead>
            <tr>
                <th class="CbxColumn">
                    <input type="checkbox" class="CheckAll" />
                </th>
                <th>
                    运单号
                </th>
                <th>
                    订单号
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.FormCodeList != null)
            {
                foreach (var item in Model.FormCodeList)
                {
                <tr>
                    <td class="CbxColumn">
                        <input type="checkbox" FormCode = "@item.FormCode" />
                    </td>
                    <td>
                        @item.FormCode
                    </td>
                    <td class="center">
                        @item.CustomerOrder
                    </td>
                </tr>
                }
            }
        </tbody>
    </table>
</div>