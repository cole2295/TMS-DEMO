@{
    ViewBag.Title = "撤回已调度单据";
    Layout = "~/Views/Shared/_Layout_Frame.cshtml";
}
@section header{
    <script type="text/javascript" src="@Url.Content("~/Scripts/references/jquery.unobtrusive-ajax.min.js")"></script>
    <script type="text/javascript">
        function cancel() {
            ymPrompt.doHandler("create", true, { action: 'create', result: 'cancel' })
        }
        $(function () {
            $("#txtDeliveryNo").focus();
        })
    </script>
}
@using (Ajax.BeginForm(new AjaxOptions() { HttpMethod = "Post", LoadingElementId = "Loading" }))
{
    <div id="UpdateTarget">
        <label style="line-height: 40px; font-size: 14px;">
            请输入状态为“发货调度完成”的提货单号
        </label>
        <div class="center">
            <input id="txtDeliveryNo" name="DeliveryNo" type="text" style="width: 200px; height: 32px;
                line-height: 32px; font-size: 26px; margin: 15px auto;" /></div>
        <div class="split">
        </div>
        <div class="center">
            <input type="submit" class="button" value="确认撤回" />
            <input type="button" class="button" value="取消" onclick="cancel()" />
        </div>
    </div>
}