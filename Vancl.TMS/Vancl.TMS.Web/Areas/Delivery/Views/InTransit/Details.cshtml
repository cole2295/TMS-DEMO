@using Vancl.TMS.Web.WebControls.Mvc;
@model Vancl.TMS.Model.Delivery.InTransit.ViewInTransitModel
@{
    ViewBag.Title = "查看运输详细信息";
    Layout = "~/Views/Shared/_Layout_Frame.cshtml";
}
@section header{
}
<table class=" tb_insert">
    <thead>
        <tr>
            <th colspan="2">
                @Html.HiddenFor(model => model.DispatchID)
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th style="width: 110px;">
                提货单号
            </th>
            <td>
                @Model.DeliveryNo
            </td>
            <th style="width: 110px;">
                物流单号
            </th>
            <td>
                @Model.WaybillNo
            </td>
        </tr>
        <tr>
            <th>
                创建时间
            </th>
            <td>
                @Model.CreateTime.ToString("yyyy-MM-dd")
            </td>
            <th>
                出发地
            </th>
            <td style="">
                @Model.DepartureName
            </td>
        </tr>
        <tr>
            <th>
                目的地
            </th>
            <td>
                @Model.ArrivalName
            </td>
            <th>
                目的城市
            </th>
            <td>
                @Model.ArrivalCity
            </td>
        </tr>
        <tr>
            <th>
                中转站
            </th>
            <td>
            </td>
            <th>
                箱数
            </th>
            <td>
                @Model.BoxCount
            </td>
        </tr>
        <tr>
            <th>
                承运商
            </th>
            <td>
                @Model.CarrierName
            </td>
            <th>
                运输方式
            </th>
            <td>
                 @(Vancl.TMS.Util.EnumUtil.EnumHelper.GetDescription<Vancl.TMS.Model.Common.Enums.DeliveryStatus>(@Model.DeliveryStatus.ToString()))
            </td>
        </tr>
        <tr>
            <th>
                到货时效
            </th>
            <td>
            @Model.ArrivalTiming
            </td>
            <th>
                违禁类型
            </th>
            <td>
                 @(Vancl.TMS.Util.EnumUtil.EnumHelper.GetDescription<Vancl.TMS.Model.Common.Enums.DeliveryStatus>(@Model.LineGoodsType.ToString()))
            </td>
        </tr>
        <tr>
            <th>
                总价
            </th>
            <td>
            @Model.TotalAmount
            </td>
            <th>
                预计到货时间
            </th>
            <td>
            @Model.ExpectArrivalDate.ToString("yyyy-MM-dd HH:mm")
            </td>
        </tr>
        <tr>
            <th>
                提货单状态
            </th>
            <td>
                 @(Vancl.TMS.Util.EnumUtil.EnumHelper.GetDescription<Vancl.TMS.Model.Common.Enums.DeliveryStatus>(@Model.DeliveryStatus.ToString()))
            </td>
            <th>
                时效
            </th>
            <td>
            @Model.ArrivalTiming
            </td>
        </tr>
@*
        <tr>
            <th>
                预计延误说明
            </th>
            <td>
            </td>
            <th>
                延误类型
            </th>
            <td>
            </td>
        </tr>
        <tr>
            <th>
                修正到货日期
            </th>
            <td>

            </td>
            <th>
                审批状态
            </th>
            <td>
       
            </td>
        </tr>
*@
    </tbody>
</table>
<div class="split">
</div>
<div class="center">
    @*<input type="button" class="button" value="返回列表" onclick='location="@Url.Action("List")"' />*@
</div>
<div>
